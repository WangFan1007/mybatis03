<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.wang.dao.ICountry">
    <select id="getCounreyByID" parameterType="int" resultType="com.wang.domain.Country">
        select * from `country` where id = #{id}
    </select>

    <select id="getAll" resultType="com.wang.domain.Country">
        select * from `country`
    </select>


</mapper>